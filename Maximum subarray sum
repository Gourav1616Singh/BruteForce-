class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int i;
        int max=-100000;
        for(i=0;i<nums.size();i++)
        {
            int sum=0;
        for(int j=i;j<nums.size();j++)
        {
            sum=sum+nums[j];
             if (sum> max)
                max=sum;
        }
           
        }
            return max;
            
        
    }
};
