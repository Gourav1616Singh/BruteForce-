class Solution {
public:
    int findDuplicate(vector<int>& nums) {
        unordered_map<int,int>mp;
       
        int ans=0;
        for(int i=0;i<nums.size();i++)
        {
          mp[nums[i]]++;
        }
        int n;
         for(auto it:mp){
        if(it.second>1)
           return it.first;
            }

    return 0;    
        
    }
    
        
    
   
};
